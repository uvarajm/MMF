  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "start:storybook": "storybook dev -c .storybook -p 9000",
    "build-storybook": "storybook build",
    "lint": "eslint --ext .jsx --ext .js ./src --debug --cache --fix --quiet && npm run lint-styles",
    "lint-styles": "stylelint ./src/views/components/**/*.scss --fix",
    "lint:docs": "eslint --ext .jsx --ext .js ./src --debug --cache --fix --output-file docs/code_quality/index.html",
    "lint:sonar": "eslint --ext .jsx --ext .js ./src -f json -o docs/eslint/report.json",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --collectCoverage --watchAll=false --detectOpenHandles --no-cache --coverageDirectory='docs/tests/'  --forceExit --collectCoverageFrom=src/**/*.js --collectCoverageFrom=!src/**/*.stories.js  --collectCoverageFrom=!src/**/Icons/*.js --collectCoverageFrom=!src/store/slices/**/*.js",
    "jsdoc:build": "npm run jsdoc:build:external",
    "jsdoc:build:external": "jsdoc -c .jsdoc.json -t ./node_modules/ink-docstrap/template  -r -R JSDOC_MAIN.md  --verbose",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:coverage && npm run jsdoc:build"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    }
  }
}
